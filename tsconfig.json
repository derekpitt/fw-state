{
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "es6",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
    "dist",
    "build-temp",
    "test"
  ]
}
